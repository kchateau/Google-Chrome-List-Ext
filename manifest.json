{
  "manifest_version": 2,
  "name": "Simple List",
  "description": "List Extension",
  "version": "1.1",

  "icons": { 
    "48": "icon.png"
   },

  "permissions": [
    "http://*/*", 
    "https://*/*",
    "storage"
  ],

  "browser_action": {
    "default_title": "Simple List Maker",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}